{
  "name": "vue-skeleton",
  "version": "0.7.4",
  "description": "A Vue Skeleton",
  "author": "Hendrik-Jan de Harder <hendrik-jan@mediamonks.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build/dev-server.js",
    "dev-dhr": "cross-env NODE_ENV=development node build/dev-server.js --disableHotReloading",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "eslint": "eslint src --ext .js --cache",
    "tslint": "tslint src/**/*.ts -c .tslintrc.js -p tsconfig.json",
    "stylelint": "stylelint \"./src/**/*.scss\" --cache",
    "preview": "node build/preview-server.js",
    "prepush": "node bin/prePush.js",
    "precommit": "lint-staged",
    "analyze": "webpack-bundle-analyzer stats.json ./dist",
    "postcommit": "git reset",
    "prettify": "prettier --write --print-width 120 --tab-width 4 --use-tabs --single-quote --trailing-comma all \"src/**/*.{js,scss,ts}\""
  },
  "lint-staged": {
    "gitDir": "./",
    "linters": {
      "src/**/*.{js,ts,scss}": [
        "prettier --write --print-width 120 --tab-width 4 --use-tabs --single-quote --trailing-comma all",
        "git add"
      ]
    }
  },
